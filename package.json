{
  "name": "Synchrotunes",
  "version": "1.0.0",
  "description": "Sync iTunes playlists with any folder",
  "main": "main.js",
  "esversion": 8,
  "scripts": {
    "start": "electron .",
    "pack:osx": "  electron-packager . --out=dist --overwrite --platform=darwin --arch=x64",
    "pack:mas": "  electron-packager . --out=dist --overwrite --platform=mas    --arch=x64",
    "pack:win32": "electron-packager . --out=dist --overwrite --platform=win32  --arch=ia32",
    "pack:win64": "electron-packager . --out=dist --overwrite --platform=win32  --arch=x64",
    "pack:lin32": "electron-packager . --out=dist --overwrite --platform=linux  --arch=ia32",
    "pack:lin64": "electron-packager . --out=dist --overwrite --platform=linux  --arch=x64",
    "pack:all": "  electron-packager . --out=dist --overwrite --all"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "del": "^3.0.0",
    "jquery": "^3.3.1",
    "xml2js": "^0.4.19"
  }
}
